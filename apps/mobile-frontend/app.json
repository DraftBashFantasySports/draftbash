{
    "expo": {
        "name": "draftbash",
        "scheme": "draftbash",
        "slug": "draftbash",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./src/assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./src/assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./src/assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.stephenfeddes.draftbash"
        },
        "web": {
            "favicon": "./src/assets/favicon.png"
        },
        "extra": {
            "eas": {
                "projectId": "872d35c7-3881-4e4f-a080-7e6593dd37ad"
            }
        },
        "plugins": ["expo-router"]
    }
}
